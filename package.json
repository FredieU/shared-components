{
  "name": "shared-components",
  "description": "A library of reusable React components",
  "version": "0.1.0",
  "private": true,
  "main": "dist/bundle.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FredieU/shared-components.git"
  },
  "scripts": {
    "build": "build-storybook -s public",
    "deploy": "firebase deploy",
    "eject": "react-scripts eject",
    "postinstall": "rm -rf dist && rollup --config",
    "start": "start-storybook -p 6006 -s public",
    "start:server": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.18",
    "@types/node": "^12.19.8",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.6",
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-import-css": "^1.1.5",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/node-logger": "^6.1.10",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.10",
    "rollup": "^2.38.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
